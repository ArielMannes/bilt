{
  "name": "@bildit/bildit-here",
  "version": "1.0.2",
  "description": "The CLI that builds a local folder",
  "main": "src/bildit-here.js",
  "files": [
    "src",
    "scripts"
  ],
  "scripts": {
    "test": "mocha --no-timeouts 'test/**/*.test.js'",
    "build": "npm run build:e2e-keys && build:e2e-git-repo",
    "build:e2e-keys": "echo y | ssh-keygen -t rsa -P '' -f test/it/bildit-here/git-server/keys/id_rsa",
    "build:e2e-git-repo": "git init --bare test/it/bildit-here/git-server/repos/test-repo"
  },
  "keywords": [
    "ci",
    "build"
  ],
  "author": "Gil Tayar <gil@tayar.org>",
  "license": "Unlicense",
  "repository": {
    "url": "https://github.com/giltayar/bildit",
    "type": "git"
  },
  "dependencies": {
    "@bildit/config-based-plugin-repository": "^1.0.6",
    "cpr": "^2.2.0",
    "debug": "^2.6.8",
    "get-npm-token": "^1.0.1",
    "git-repo-info": "^1.4.1",
    "ignore": "^3.3.3",
    "jest-changed-files": "^20.0.3",
    "make-dir": "^1.0.0",
    "set-union": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "docker-compose-mocha": "^1.1.0",
    "eslint": "^4.7.0",
    "eslint-plugin-prettier": "^2.1.2",
    "mocha": "^3.5.3",
    "prettier": "^1.7.0",
    "test-fixture": "^1.0.2"
  }
}
