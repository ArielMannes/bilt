{
  "name": "@bilt/bilt-cli",
  "version": "1.0.2",
  "description": "The CLI that builds a local folder",
  "main": "src/bilt-cli.js",
  "files": [
    "src",
    "scripts"
  ],
  "scripts": {
    "test": "mocha --no-timeouts 'test/**/*.test.js' && npm run eslint",
    "eslint": "eslint '**/*.js'",
    "build": "npm run build:e2e-keys && build:e2e-git-repo",
    "build:e2e-keys": "echo y | ssh-keygen -t rsa -P '' -f test/it/bilt-cli/git-server/keys/id_rsa",
    "build:e2e-git-repo": "git init --bare test/it/bilt-cli/git-server/repos/test-repo"
  },
  "keywords": [
    "ci",
    "build"
  ],
  "author": "Gil Tayar <gil@tayar.org>",
  "license": "Unlicense",
  "repository": {
    "url": "https://github.com/giltayar/bilt",
    "type": "git"
  },
  "dependencies": {
    "@bilt/artifact-finder": "^1.0.9",
    "cosmiconfig": "^3.1.0",
    "cpr": "^2.2.0",
    "debug": "^2.6.9",
    "get-npm-token": "^1.0.1",
    "git-repo-info": "^1.4.1",
    "ignore": "^3.3.3",
    "jest-changed-files": "^20.0.3",
    "make-dir": "^1.0.0",
    "plugin-import": "^1.0.7",
    "set-union": "^1.0.0",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "docker-compose-mocha": "^1.2.0",
    "eslint": "^4.8.0",
    "eslint-plugin-mocha-no-only": "0.0.5",
    "eslint-plugin-prettier": "^2.3.1",
    "mocha": "^3.5.3",
    "prettier": "^1.7.4",
    "test-fixture": "^1.0.2"
  }
}
