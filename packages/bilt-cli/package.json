{
  "name": "@bilt/bilt-cli",
  "version": "2.0.7",
  "description": "The CLI that builds a local folder",
  "main": "src/bilt-cli.js",
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "src",
    "scripts"
  ],
  "bin": {
    "bilt": "scripts/run-bilt-cli.js"
  },
  "scripts": {
    "test": "npm run eslint && npm run test:mocha-parallel",
    "test:mocha": "mocha --no-timeouts --exit 'test/unit/*.test.js' 'test/it/*.test.js' 'test/e2e/*.test.js'",
    "test:mocha-parallel": "mocha-parallel-tests --no-timeouts --exit 'test/unit/*.test.js' 'test/it/*.test.js' 'test/e2e/*.test.js'",
    "eslint": "eslint '**/*.js'",
    "build": "npm run build:e2e-keys && npm run build:e2e-git-repo",
    "build:e2e-keys": "echo y | ssh-keygen -t rsa -P '' -f test/it/bilt-cli/git-server/keys/id_rsa",
    "build:e2e-git-repo": "git init --bare test/it/bilt-cli/git-server/repos/test-repo"
  },
  "keywords": [
    "ci",
    "build"
  ],
  "author": "Gil Tayar <gil@tayar.org>",
  "license": "Unlicense",
  "repository": {
    "url": "https://github.com/giltayar/bilt",
    "type": "git"
  },
  "dependencies": {
    "@bilt/artifact-finder": "^2.0.27",
    "@bilt/host-agent": "^3.0.30",
    "@bilt/in-memory-events": "^2.0.30",
    "@bilt/in-memory-job-dispatcher": "^1.0.52",
    "@bilt/jobs": "^2.0.27",
    "@bilt/last-build-info": "^4.0.18",
    "@bilt/npm-build-job": "^2.0.21",
    "@bilt/repo-build-job": "^2.0.23",
    "@bilt/symlink": "^2.0.32",
    "chalk": "^2.4.2",
    "cosmiconfig": "^5.2.1",
    "debug": "^4.1.1",
    "git-repo-info": "^2.1.0",
    "ignore": "^5.1.4",
    "is-ci": "^2.0.0",
    "jest-changed-files": "^23.4.2",
    "lodash.flatten": "^4.4.0",
    "lodash.pickby": "^4.6.0",
    "mocha-parallel-tests": "^2.2.2",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cpr": "^3.0.1",
    "docker-compose-mocha": "^1.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.2.1",
    "prettier": "^1.18.2",
    "test-fixture": "^2.4.1"
  }
}
